int main() {
    ios_base::sync_with_stdio(0);cin.tie(0);
    //  ofstream fout("");
    //  ifstream fin("");
    vi v;
    int n;
    int c = 0;
    while (while cin >> n) {
      v.pb(n);
      c++;
      if (c%2 == 1) {
        nth_element(v.begin(), v.begin() + c/2,v.end());
        cout << v[c/2] << endl;
      }
      else {
                nth_element(v.begin(), v.begin() + c/2,v.end());
                int a1 = v[c/2];
        nth_element(v.begin(), v.begin() + c/2-1,v.end());
        int a2 = v[c/2-1];
        cout << (a1+a2)/2 << endl;
      }
    }
